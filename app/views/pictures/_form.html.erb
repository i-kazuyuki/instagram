<%= form_for(@picture) do |f| %>
  <%= f.error_messages! %>

  <div class="field">
    <%= f.label :アップロード写真 %>
    <%= f.file_field :photo %>
    <%= f.hidden_field :photo_cache %>
  </div>
  <div class="field">
    <%= f.label :コメント %>
    <%= f.text_field :content, class: 'form-control' %>
  </div>
  <%= f.submit '投稿', class: 'btn btn-primary btn-block' %>
<% end %>

<%= link_to "キャンセル", pictures_path %>
